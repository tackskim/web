	<!-- 2018-08-30 js 추가 영역 -->
	<script type="text/javascript">

		var dimm_count = 0;

		$(document).ready(function(){



			var fnSetLayer ={
				init : function(){
					if(dimm_count==0){
						document.getElementById("main-layer-dimm").style.display = "none";
					}
					this.ban_w = $('.main-layer-banner').width();
					this.ban_h = $('.main-layer-banner').height();

					this.addEvent();
					this.resizePop();
				},
				addEvent : function(){
					$(window).resize(function(){
							fnSetLayer.resizePop();	// 화면 리사이즈 시 팝업 사이즈 조정
					});
				},
				resizePop : function(){
					var win_w = $(window).width();		// window가로 사이즈가 세로 사이즈보다
					var win_h = $(window).height();
					if(this.ban_w > win_w){
						$('.main-layer-banner').addClass('mobile');
					}else if(this.ban_h > win_h){
						$('.main-layer-banner').addClass('mobile');
					}else{
						$('.main-layer-banner').removeClass('mobile');
					}
				}
			}

			$('.main-layer-banner').length&&fnSetLayer.init();

		});

		function fncClose(value){
			if($("#oneDayCheck"+value).is(':checked')){
				setCookieOneDay("urgencyNotice"+value, "no" , 1);
			}
			$('#main-popup'+value).css('display', 'none');

			dimm_count--;

			if(dimm_count == 0){
				document.getElementById("main-layer-dimm").style.display = "none";
			}
		}

		function setCookieOneDay( name, value, expiredays ) {
			var todayDate = new Date();
			todayDate.setDate( todayDate.getDate() + expiredays );
			document.cookie = name + "=" + escape( value ) + "; path=/; expires=" + todayDate.toGMTString() + ";"
		}
		function getCookie( name ) {
			var nameOfCookie = name + "=";
			var x = 0;
			while ( x <= document.cookie.length ) {
				var y = (x+nameOfCookie.length);
				if ( document.cookie.substring( x, y ) == nameOfCookie ) {
					if ( (endOfCookie=document.cookie.indexOf( ";", y )) == -1 )
						endOfCookie = document.cookie.length;
					return unescape( document.cookie.substring( y, endOfCookie ) );
				}
				x = document.cookie.indexOf( " ", x ) + 1;
				if ( x == 0 )
					break;
			}
			return "";
		}
	</script>
