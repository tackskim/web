	<script>
	//조회
	function fn_search(){
		document.searchForm.action="/career/recruit?flag=1";
		document.searchForm.submit();
	}

	//조회
	function fn_ajaxSearch(){
		//if(Number($("#cpage").val())*5 >= Number("")){
		//	alert("더이상 데이터가 없습니다.");
		//	return;
		//}

		$.ajax({
			url: "/career/recruitAjax",
			type: "POST",
			data: {
					  "career" 	: "NE"
					 ,"cpage"	: Number($("#cpage").val())+1
					 ,"selected": ""
					 ,"searchTxt": ""
				  },
			dataType: "json",
			success: function(data){
				//alert(JSON.stringify(data));

				var innerHtml = "";
				for(var i=0;i<data.list.length;i++){
					innerHtml +='<li>';
					innerHtml +='<a href="/career/recruit_view?idx='+data.list[i].idx+'" class="recruitSummary">';

					innerHtml +='<p class="title dot">'+data.list[i].title+'</p>';

					var dutyStr = "";
					if(data.list[i].duty == 'PL'){
						dutyStr = "경영지원 - 기획/관리";
					}else if(data.list[i].duty == 'FI'){
						dutyStr = "경영지원 - 재무";
					}else if(data.list[i].duty == 'MA'){
						dutyStr = "콘텐츠 - 기획/제작";
					}else if(data.list[i].duty == 'AD'){
						dutyStr = "콘텐츠 - 제작지원/부가사업";
					}

					var careerStr = "";
					if(data.list[i].career=="NE"){
						careerStr = "신입";
					}else if(data.list[i].career=="CA"){
						careerStr = "경력";
					}else if(data.list[i].career=="IN"){
						careerStr = "인턴";
					}else if(data.list[i].career=="NE/CA"){
						careerStr = "신입/경력";
					}else if(data.list[i].career=="NE/IN"){
						careerStr = "신입/인턴";
					}else if(data.list[i].career=="CA/IN"){
						careerStr = "경력/인턴";
					}else{
						careerStr = "신입/경력/인턴";
					}

					var strDay = "";
					var strCls = "";
					if(data.list[i].all_day=="0"){
						if(data.list[i].dDay=='0'){
							strDay = "D-Day";
							strCls = "today";
						}else{
							strCls = "";
							strDay = "D-"+data.list[i].dDay;
						}
					}else{
						strCls = "";
						strDay = "상시";
					}


					var applStr="";
					if(data.list[i].appl_start != null && data.list[i].appl_start != ""){
						applStr =data.list[i].appl_start+' ~ '+data.list[i].appl_end;
					}
					innerHtml +='<p class="division"><span class="career">'+careerStr+'</span><span class="part">'+dutyStr+'</span></p>';
					innerHtml +='<p class="period">'+applStr+'</p>';
					innerHtml +='<p class="date '+strCls+'">'+strDay+'</p>';

					innerHtml +='</a>';
					innerHtml +='</li>';
				}
				$("#dataUl").append(innerHtml);
				$("#cpage").val(Number($("#cpage").val())+1);

				if( parseInt("") <= (parseInt($("#cpage").val())*parseInt("5")) )
					document.getElementById("btnMore").style.display = "none";
			},
			error		:
				function (jqXHR, textStatus, errorThrown) {
					isProcess = false;
					alert('오류가 발생했습니다\n[' + textStatus + ']\n' + errorThrown);
			}
		});
	}

	if(""== "" ||  parseInt("") <= (parseInt("1")*parseInt("5")) )
		document.getElementById("btnMore").style.display = "none";
	</script>
