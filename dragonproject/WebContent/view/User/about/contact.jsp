		<script type="text/javascript">
			var pos = new naver.maps.LatLng(37.5784652, 126.8929289);

			var mapOptions = {
				center : pos,
				zoom : 11,
				mapTypeControl : true,
				zoomControl : true,
				logoControl : false,
				zoomControlOptions : {
					style : naver.maps.ZoomControlStyle.DROPDOWN,
					position : naver.maps.Position.LEFT_TOP
				}
			};

			var map = new naver.maps.Map('mapArea', mapOptions);

			var pcIcon = {
				url : '/resources/front/kr/images/about/img_position.png',//못찾음
				size : new naver.maps.Size(186, 101),
				scaledSize : new naver.maps.Size(186, 101),
				origin : new naver.maps.Point(0, 0),
				anchor : new naver.maps.Point(107, 101)
			}

			var mobileIcon = {
				url : 'resources/front/kr/images/about/img_position.png',//못찾음
				size : new naver.maps.Size(93, 50),
				scaledSize : new naver.maps.Size(93, 50),
				origin : new naver.maps.Point(0, 0),
				anchor : new naver.maps.Point(53, 50)
			}

			var marker = new naver.maps.Marker({
				position : pos,
				map : map,
				animation : naver.maps.Animation.DROP
			});

			setIcon();
			map.panBy(new naver.maps.Point(0, -25)); // 하단으로 25 픽셀 이동

			var wmode;

			window.onresize = function(e) {
				var currentMode;

				if (window.innerWidth > 768) {
					currentMode = "pc";
				} else {
					currentMode = "mobile";
				}

				if (wmode != currentMode) {
					setIcon();
					wmode = currentMode;
				}
			}

			function setIcon() {
				if (window.innerWidth > 768) {
					marker.setIcon(pcIcon);
				} else {
					marker.setIcon(mobileIcon);
				}
			}
		</script>
